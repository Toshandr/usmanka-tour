{
  "name": "usmanka-weekend",
  "version": "1.0.0",
  "description": "Активные туры выходного дня на природе Воронежской области",
  "main": "server/index.js",
  "scripts": {
    "dev": "concurrently \"npm run server\" \"npm run client\"",
    "server": "nodemon server/index.js",
    "client": "cd client && npm start",
    "build": "cd client && npm run build",
    "start": "node server/index.js",
    "install-all": "npm install && cd client && npm install",
    "test-email": "node test-email.js",
    "build:static": "cd client && npm run build",
    "deploy:gh-pages": "cd client && npm run deploy"
  },
  "keywords": [
    "tours",
    "weekend",
    "voronezh",
    "usmanka"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "body-parser": "^1.20.2",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "ejs": "^3.1.10",
    "express": "^4.18.2",
    "nodemailer": "^6.9.7"
  },
  "devDependencies": {
    "concurrently": "^8.2.2",
    "nodemon": "^3.0.1"
  }
}
